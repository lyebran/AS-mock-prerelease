#Task 2

task2 = open("monsters.txt", "r")
found = False
prompt = input("Enter Monster (Case Sensitive): ")
while True:
    line = task2.readline()
    eachline = line.split(",")
    if len(line) == 0:
        break
    elif str(prompt) == eachline[1]:
        found = True

if found == True:
    print("Monster Found")
else:
    print("Monster Not Found")
task2.close()
